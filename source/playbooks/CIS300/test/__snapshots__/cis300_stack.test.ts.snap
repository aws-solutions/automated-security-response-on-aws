def parse_non_string_types(param):
    if re.match("^\\d+$", str(param)):
        param = int(param)
        return param
    if str(param).lower() == "true":
        return True
    if str(param).lower() == "false":
        return False
    if isinstance(param, list):
        return param
    if len(param.split(",")) > 1:
        return param.split(",")
    return param


